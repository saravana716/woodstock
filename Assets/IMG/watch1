			<script>!function(){var n="active",t="passive",e="hidden",i=[],r=0,a=!1,o=!1;function d(){return performance.now()}function u(){if(a){var n=d(),t={startTs:r,endTs:n,duration:n-r};a=!1,i.push(t)}}function s(){a||(r=d(),a=!0)}function c(){var i=document.visibilityState===e?e:document.hasFocus()?n:t;i===t?o?s():u():i===n?s():u()}function f(){u();var n=i.reduce(function(n,t){return n+t.duration},0);(n=Math.floor(n))>0&&window.trackingUtil&&trackingUtil("VI_DWELL_TIME","ex1",Math.floor(n))}function v(n){n.persisted&&(a=!1,i=[],c())}addEventListener("focus",c,!0),addEventListener("blur",c,!0),addEventListener("visibilitychange",c,!0),addEventListener("beforeunload",f,!0),addEventListener("pageshow",v),c()}();</script>
